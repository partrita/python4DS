  {
   "cell_type": "code",
   "execution_count": null,
   "id": "67c861b6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rescale(x):\n",
    "    x = x.replace(np.inf, np.nan)\n",
    "    minimum = x.min()\n",
    "    return (x - minimum) / (x.max() - minimum)\n",
    "\n",
    "\n",
    "rescale(pd.Series([1, 2, 3, np.inf, 5]))"
   ]
